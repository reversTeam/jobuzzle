#import (Config.path +'/Kernel/Route/MasterRoute') = MasterRoute;

class ExampleAuthorMainRoute extends MasterRoute {

	name = 'author';

	baseName = 'Author';
	baseRoute = '{authors}';
	baseController = 'example_author_main_controller';

	paramName = 'author_url';
	paramRegex = '[a-z0-9-]+';

}

#export ExampleAuthorMainRoute;