#import (Config.path +'/Kernel/Service/MasterService') = MasterService;

class ExampleBookService extends MasterService {

	_author = {};

	#default get, set for _author;

	initialize : function () {
		_super();
		console.log('BookService Instancied');
	}

	getAuthorName : function () {
		var author = this.getAuthor();
		return author.getFirstName() +' '+ author.getLastName();
	}

}

#export ExampleBookService;