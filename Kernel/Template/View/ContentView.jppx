#import ('./MasterView') = MasterView;

class ContentView extends MasterView {

	/**
	* Overide render if element is not visible
	* return null
	* else call display method
	**/
	render : function () {
		if (!this._template.getVisibility())
			return null;
		return this.display();
	}

	onVisible : function() {}

}

#export ContentView;